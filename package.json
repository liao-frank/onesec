    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "gh-pages": "^3.1.0",
    "microbundle": "^0.12.4",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.29.0",
